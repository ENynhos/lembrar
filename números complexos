#include <stdio.h>

typedef struct{
    float fr;
    float im;
} comp;

void scanC(comp *c){
    scanf("(%f %f'i')", &c->fr, &c->im);
    
}

void printC(comp c){
    printf("(%f %+fi)", c.fr, c.im);
    
comp somC(comp a, comp b){
    comp s;
    s.fr = a.fr + b.fr;
    s.im = a.im + b.im;
    return s;
}

comp subC(comp a, comp b){
    comp sb;
    sb.fr = a.fr - b.fr;
    sb.im = a.im - b.im;
    return sb;
}

comp multC(comp a, comp b){
    comp m;
    m.fr = (a.fr * b.fr) - (a.im * b.im);
    m.im = (a.fr * b.im) + (b.fr * a.im);
    return m;
}

comp divC(comp a, comp b)
